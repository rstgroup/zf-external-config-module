{
    "name": "rstgroup/zf-external-config-module",
    "description": "Module responsible for fetching configuration from external sources.",
    "license": "MIT",
    "authors": [
        {
            "name": "Artur Wolny",
            "email": "artur.wolny@rst.com.pl"
        }
    ],
    "autoload": {
        "psr-4": {
            "RstGroup\\ZfExternalConfigModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RstGroup\\ZfExternalConfigModule\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^5.6 || ^7",
        "zendframework/zend-modulemanager": "^2.7",
        "zendframework/zend-servicemanager": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "unit-tests": "phpunit",
        "unit-tests-coverage": "phpunit --coverage-html ./results"
    }
}
